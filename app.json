      "edgeToEdgeEnabled": true,
      "package": "com.starshield.TaskManagerApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a91633d0-3bfe-466b-9058-45b8f77be384"
